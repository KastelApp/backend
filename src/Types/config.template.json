					"type": "object"
				},
				"nodes": {
					"items": {
						"type": "string"
					},
					"type": "array"
				},
				"password": {
					"type": "string"
				},
				"username": {
					"type": "string"
				}
			},
			"type": "object",
			"required": ["durableWrites", "keyspace", "networkTopologyStrategy", "nodes", "password", "username"]

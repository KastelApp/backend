									"type": ["string", "number"]
								},
								"leeway": {
									"type": ["string", "number"]
								}
							}
						},
						"closeTimeout": {
							"type": "object",
							"properties": {
								"interval": {
									"type": ["string", "number"]
								},
								"leeway": {
									"type": ["string", "number"]

			"required": [
				"bucketInterval",
				"bucketRnd",
				"captchaEnabled",
				"cloudflareAccessOnly",
				"domain",
				"features",
				"localIps",
				"port",
				"secure",
				"sentry",
				"strictRouting",
				"turnstileSecret",
				"workerId"

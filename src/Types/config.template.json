				"DisableEmailVerification",
				"DisableGuildCreation",
				"DisableLogin",
				"DisablePasswordReset",
				"DisableRegistration",
				"InviteBasedRegistration"
			],
			"type": "string"
		},
		"MailServer": {
			"properties": {
				"enabled": {
					"type": "boolean"
				},
				"users": {
					"items": [
						{
							"$ref": "#/definitions/User"
						},
						{
							"$ref": "#/definitions/User"
						}
					],
					"maxItems": 2,
					"minItems": 2,
					"type": "array"
				}
			},
			"type": "object",
			"required": ["enabled", "users"]
		},
		"Redis": {
			"properties": {
				"db": {
					"type": "number"
				},
				"host": {
					"type": "string"
				},
				"password": {
					"type": "string"
				},
				"port": {
					"type": "number"
				},
				"username": {
					"type": "string"
				}
			},
			"type": "object",
			"required": ["db", "host", "password", "port", "username"]
		},
		"ScyllaDB": {
			"properties": {
				"durableWrites": {
					"type": "boolean"
				},
				"keyspace": {
					"type": "string"

		},
		"redis": {
			"$ref": "#/definitions/Redis"
		},
		"scyllaDB": {
			"$ref": "#/definitions/ScyllaDB"
		},
		"server": {
			"$ref": "#/definitions/Server"
		},
		"ws": {
			"$ref": "#/definitions/Ws"

{
    "name": "kastel",
    "description": "A Chatting app like Discord or Guilded.",
    "version": "0.0.5",
    "main": "src/index.js",
    "dependencies": {
        "bcrypt": "^5.0.1",
        "body-parser": "^1.20.0",
        "chalk": "^4.1.2",
        "commander": "^9.4.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.0.2",
        "express": "^4.18.2",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.5.4",
        "ms": "^2.1.3",
        "redis": "^4.3.1",
        "speakeasy": "^2.0.0",
        "ws": "^8.8.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.14",
        "eslint": "^8.23.0",
        "nodemon": "^2.0.15"
    },
    "scripts": {
        "start": "node src/index.js",
        "lint": "eslint 'src/**'",
        "db_check": "if [ ! -d db ]; then mkdir db; echo \"DB Directory created\"; else echo \"DB Directory exists\"; fi",
        "db_redis_start": "redis-server --port 6379 --daemonize yes --dir /Kastel/db/redis",
        "db_mongo_start": "mongod --dbpath /Kastel/db/mongo --port 27017 --fork --logpath /Kastel/db/mongo/mongo.log",
        "db": "npm run db_check && npm run db_redis_start & npm run db_mongo_start & sleep 5 && npm run start"
    },
    "keywords": [
        "chatting"
    ],
    "engines": {
        "node": "^17.x.x",
        "npm": "^8.x.x"
    },
    "author": "DarkerInk",
    "license": "GPL-3.0-or-later",
    "types": "index.d.ts"
}
